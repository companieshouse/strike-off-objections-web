    "ch-node-session-handler": "git+ssh://git@github.com/companieshouse/node-session-handler.git#3.0.1",
    "ch-sdk-node": "git+ssh://git@github.com/companieshouse/ch-sdk-node.git#0.2.10",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "form-data": "^2.3.3",
    "govuk-frontend": "^3.6.0",
    "ioredis": "^4.17.3",
    "js-yaml": "^3.14.0",
    "moment": "^2.24.0",
    "nunjucks": "^3.2.1",
    "utf-8-validate": "^5.0.2",
    "web-security-node": "git+ssh://git@github.com/companieshouse/web-security-node.git#0.1.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
